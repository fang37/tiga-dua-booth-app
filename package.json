{
  "type": "module",
  "name": "tiga-dua-booth-app",
  "productName": "tiga-dua-booth-app",
  "version": "1.0.3",
  "description": "My Electron application description",
  "main": "src/main/main.js",
  "scripts": {
    "dev": "vite",
    "start": "electron .",
    "build": "electron-builder",
    "dist": "electron-builder --win --x64",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "mirfa",
  "license": "MIT",
  "devDependencies": {
    "electron": "38.1.0",
    "electron-builder": "^24.13.3",
    "vite": "^5.4.20"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "better-sqlite3": "^12.2.0",
    "chokidar": "^4.0.3",
    "dotenv": "^17.2.2",
    "electron-squirrel-startup": "^1.0.1",
    "googleapis": "^160.0.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-cropper": "^2.3.3",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.3"
  },
  "build": {
    "appId": "com.tigaduabooth.app",
    "productName": "tiga-dua-booth-app",
    "asar": true,
    "asarUnpack": ["**/node_modules/sharp/**", "**/node_modules/@img/**/*"],
    "files": [
      "src/**/*",
      "node_modules/**/*",
      "package.json",
      "credentials.json"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}